<main>
  <section id="account_section">
    <%= render 'shared/sidebar' %>
    <div id="school_pin" class="main_side">
      <div id="top_header">
        <h1>Pin</h1>
        <%= render 'shared/header-side' %>
      </div>
      <div class="row">
        <div class="col-3">
          <form action="submit">
            <div class="form_blk">
              <label for="">Name</label>
              <input type="text" class="input">
            </div>
            <div class="form_blk">
              <label for="">Longtitude</label>
              <input type="text" class="input">
            </div>
            <div class="form_blk">
              <label for="">Latitude</label>
              <input type="text" class="input">
            </div>

            <div class="btn_blk">
              <button type="button" class="btn-primary">Submit</button>
            </div>

          </form>
        </div>
        <div class="col-9">
          <div class="map_blk">
            <img src="/assets/map.jpg" alt="">
          </div>
        </div>
      </div>
    </div>
  </section>
  <!-- dash -->


</main>