  <section id="account_section">
    <%= render "shared/sidebar" %>
    <div id="buy_house" class="main_side">
      <div id="top_header">
        <h1>Sell Vacant Land</h1>
        <%= render "shared/header-side" %>
      </div>
      <div class="row">
        <div class="col-6">
          <ul class="top_blk">
            <li>
              <div class="dropdown">
                <button class="filter text dropdown-toggle fs_5 fw_600" data-bs-toggle="dropdown">
                  <div class="img_blk">
                    <%= image_tag 'filter.svg', alt: '' %>
                  </div>
                  filters
                </button>
                <%= render "shared/drop_down" %>
              </div>
            </li>
            <li>
              <div class="form_blk">
                <div class="icon_blk">
                  <%= image_tag 'search.svg', alt: '' %>
                </div>
                <input type="search" placeholder="Search" class="input">
              </div>
            </li>
            <li>
              <button type="button" class="filter">
                <div class="img_blk">
                  <%= image_tag 'csv-file-icon.svg', alt: '' %>
                </div>
                <%= link_to "Download CSV", sell_vacant_land_users_data_path(format: 'csv', search: params[:search], page: params[:page], checkbox_value:""), id: "downloadbtncustom" %>
              </button>
            </li>
          </ul>
        </div>
      </div>
      <div class="table_blk">
        <table class="mt-5  ">
          <thead>
          <tr>
            <th style="width: 18.9rem">
              Full Name
            </th>
            <th style="width: 14.4rem">
              Property Type
            </th>
            <th style="width: 15.2rem">
              Post Title
            </th>
            <th style="width: 14.3rem">
              Asking Price
            </th>
            <th style="width: 26.2rem">
              Street Address
            </th>
            <th style="width: 14.2rem">
            Lot Depth
          </th>
          <th style="width: 14.2rem">
            Lot Depth Unit
          </th>
          <th style="width: 16.1rem">
            Lot Frontage
          </th>
          <th style="width: 16.1rem">
            Lot Frontage Unit
          </th>
          <th style="width: 14.8rem">
            Lot Size
          </th>
          <th style="width: 14.8rem">
            Lot Size Unit
          </th>
            <th style="width: 19.4rem">
              Is Your Lot Irregular
            </th>
            <th style="width: 54.3rem">
              Lot Description
            </th>
            <th style="width: 18.4rem">
              Property Taxes
            </th>
            <th style="width: 12.1rem">
              Tax Year
            </th>
            <th style="width: 50rem">
              Property Description
            </th>
          </tr>
          </thead>
          <tbody>
            <% @vacant_lands.each do |vacant_land|%>
          <tr>
          <td class="fw_500 user_name "><% vacant_land.user.full_name%></td>
          <td><%= vacant_land.type%></td>
          <td><%= vacant_land.title%></td>
          <td style="color: #00A66C;"><%= vacant_land.price%></td>
          <td><%= vacant_land.address%>5</td>
          <td><%= vacant_land.lot_depth%></td>
          <td><%= vacant_land.lot_depth_unit%></td>
          <td><%= vacant_land.lot_frontage%></td>
          <td><%= vacant_land.lot_frontage_unit%></td>
          <td><%= vacant_land.lot_size%></td>
          <td><%= vacant_land.lot_size_unit%></td>
          <td><%= vacant_land.is_lot_irregular%></td>
          <td><%= vacant_land.lot_description%></td>
          <td><%= vacant_land.property_tax%></td>
          <td><%= vacant_land.tax_year%></td>
          <td><%= vacant_land.property_description%></td>
          </tr>
          <%end%>
          </tbody>
        </table>
      </div>
      <%= render 'shared/pagination' ,data: @vacant_lands%>
    </div>
  </section>